<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequence for Đề xuất tài sản -->
        <record id="seq_tai_san_de_xuat" model="ir.sequence">
            <field name="name">Đề xuất tài sản</field>
            <field name="code">tai.san.de.xuat</field>
            <field name="prefix">DX/%(year)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence for Cấp phát tài sản -->
        <record id="seq_tai_san_cap_phat" model="ir.sequence">
            <field name="name">Cấp phát tài sản</field>
            <field name="code">tai.san.cap.phat</field>
            <field name="prefix">CP/%(year)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence for Mua tài sản -->
        <record id="seq_tai_san_mua_tai_san" model="ir.sequence">
            <field name="name">Hóa đơn mua tài sản</field>
            <field name="code">tai.san.mua.tai.san</field>
            <field name="prefix">HD/%(year)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence for Mã tài sản -->
        <record id="seq_ma_tai_san" model="ir.sequence">
            <field name="name">Mã tài sản</field>
            <field name="code">tai.san.tai.san</field>
            <field name="prefix">TS-</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
